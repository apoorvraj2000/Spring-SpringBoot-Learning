<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">



	<bean id="v1" class="com.psl.training.Vehicle" autowire="byType">
	<property name="name" value="pulsor"></property>
		<property name="color" value="Black"></property>
	</bean>

	<bean id="v2" class="com.psl.training.Vehicle" autowire="byType">
		<property name="name" value="Shine"></property>
		<property name="color" value="Red"></property>
	</bean>
	
	<bean id="eng" class="com.psl.training.model.Engine">
		<property name="capacity" value="150cc"></property>
	</bean>
</beans>